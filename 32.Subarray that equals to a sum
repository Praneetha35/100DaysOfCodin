def subarray(arr,sum1):
    dict1={}
    curr_sum = 0
    for i in range(len(arr)):
        curr_sum=curr_sum+arr[i]
        if(curr_sum==sum1):
            print("The subarray is from index 0 to ",i )
            break
        if(curr_sum-sum1 in dict1):
            print("The subarray is from index ",dict1[curr_sum-sum1]+1," to ",i )
            break
        dict1[curr_sum]=i
        
        
    print("No subarray")

n=int(input())
arr=[]
for i in range(0,n):
    a=int(input())
    arr.append(a)

sum1=int(input("Enter sum:"))
    
subarray(arr,sum1)
