#Finding repeated numbers in an array
METHOD-1:

arr=[1,1,4,3,2,5,2]
arr=sorted(arr)
rl=[]
k=0
for i in arr:
    k=k+1
    for j in range(k,len(arr)):
        if(arr[j]==arr[i] and arr[j] not in rl):
            print(arr[j])
            rl.append(arr[j])
            
METHOD-2:

arr=[1,1,4,3,2,5,2]
arr=sorted(arr)
rl=[]
for i in arr:
    if(arr.count(i)>1 and i not in rl):
       
        rl.append(i)
for i in rl:
    print(i)     
