num=int(input())
rl=[]
ar=[]
temp=num
while(num!=0):
    temp=num%10
    ar.append(temp)
    num=num//10

for i in range(0,len(ar)):
    count=1
    for j in range(i+1,len(ar)):
        if((ar[i]==ar[j]) and ar[i] not in rl):
            count=count+1 
    if(ar[i] not in rl):
        print(ar[i]," : ",count)
    rl.append(ar[i])
